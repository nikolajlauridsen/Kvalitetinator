<Window x:Class="Kvalitetinator.CreateOrderWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kvalitetinator"
        mc:Ignorable="d"
        Title="Create Order" Height="160" Width="300">
    <StackPanel>
        <DockPanel Margin="10">
            <Label Content="Customer telephone"></Label>
            <TextBox Name="PhoneEntry"></TextBox>
        </DockPanel>
        
        <DockPanel Margin="10">
            <Label>Delivery date</Label>
            <DatePicker Name="DeliveryDate"></DatePicker>
        </DockPanel>
        
        <Button Content="Create order" Width="100" Name="CreateBtn"></Button>

    </StackPanel>
</Window>
